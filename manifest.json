{
  "manifest_version": 3,
  "name": "Script Lynx",
  "version": "1.0.0",
  "description": "Blocks scripts and resources on any website based on patterns and URLs",
  "permissions": [
    "storage",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "Script Lynx"
  }
}
